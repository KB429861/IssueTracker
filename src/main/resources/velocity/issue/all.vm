#parse("/velocity/layout.vm")
#@mainLayout()
<h1>Issues</h1>
<table>
    <tr>
        <th>Id</th>
        <th>Summary</th>
        <th>Author</th>
        <th>Status</th>
    </tr>
    #foreach($issue in $issues)
        <tr>
            <td>$!issue.getId()</td>
            <td>$!issue.getSummary()</td>
            <td>$!issue.getAuthor()</td>
            <td>$!issue.getStatus()</td>
        </tr>
    #end
</table>
#end
